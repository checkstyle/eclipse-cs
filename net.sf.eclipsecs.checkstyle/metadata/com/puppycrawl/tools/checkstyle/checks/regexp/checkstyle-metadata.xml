<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE checkstyle-metadata PUBLIC
"-//eclipse-cs//DTD Check Metadata 1.1//EN"
"http://eclipse-cs.sourceforge.net/dtds/checkstyle-metadata_1_1.dtd">
<checkstyle-metadata>
    <rule-group-metadata name="%Regexp.group" priority="650">

        <rule-metadata name="%Regexp.name" internal-name="Regexp" parent="TreeWalker">
            <alternative-name internal-name="com.puppycrawl.tools.checkstyle.checks.regexp.RegexpCheck"/>
            <description>%Regexp.desc</description>
            <property-metadata name="format" datatype="Regex" default-value="$^">
                <description>%Regexp.format</description>
            </property-metadata>
            <property-metadata name="message" datatype="String" default-value="">
                <description>%Regexp.message</description>
            </property-metadata>
            <property-metadata name="illegalPattern" datatype="Boolean" default-value="false">
                <description>%Regexp.illegalPattern</description>
            </property-metadata>
            <property-metadata name="duplicateLimit" datatype="Integer" default-value="-1">
                <description>%Regexp.duplicateLimit</description>
            </property-metadata>
            <property-metadata name="errorLimit" datatype="Integer" default-value="100">
                <description>%Regexp.errorLimit</description>
            </property-metadata>
            <property-metadata name="ignoreComments" datatype="Boolean" default-value="false">
                <description>%Regexp.ignoreComments</description>
            </property-metadata>
            <message-key key="illegal.regexp"/>
            <message-key key="duplicate.regexp"/>
            <message-key key="required.regexp"/>
        </rule-metadata>

        <rule-metadata name="%RegexpMultiline.name" internal-name="RegexpMultiline" parent="Checker">
            <alternative-name internal-name="com.puppycrawl.tools.checkstyle.checks.regexp.RegexpMultilineCheck"/>
            <description>%RegexpMultiline.desc</description>
            <property-metadata name="format" datatype="Regex" default-value="^$">
                <description>%RegexpMultiline.format</description>
            </property-metadata>
            <property-metadata name="message" datatype="String">
                <description>%RegexpMultiline.message</description>
            </property-metadata>
            <property-metadata name="ignoreCase" datatype="Boolean" default-value="false">
                <description>%RegexpMultiline.ignoreCase</description>
            </property-metadata>
            <property-metadata name="minimum" datatype="Integer" default-value="0">
                <description>%RegexpMultiline.minimum</description>
            </property-metadata>
            <property-metadata name="maximum" datatype="Integer" default-value="0">
                <description>%RegexpMultiline.maximum</description>
            </property-metadata>
            <property-metadata name="fileExtensions" datatype="String">
                <description>%RegexpMultiline.fileExtensions</description>
            </property-metadata>
            <message-key key="regexp.StackOverflowError"/>
            <message-key key="regexp.empty"/>
            <message-key key="regexp.exceeded"/>
            <message-key key="regexp.minimum"/>
        </rule-metadata>

        <rule-metadata name="%RegexpOnFilename.name" internal-name="RegexpOnFilename" parent="Checker">
            <alternative-name internal-name="com.puppycrawl.tools.checkstyle.checks.regexp.RegexpOnFilenameCheck"/>
            <description>%RegexpOnFilename.desc</description>
            <property-metadata name="folderPattern" datatype="Regex">
                <description>%RegexpOnFilename.folderPattern</description>
            </property-metadata>
            <property-metadata name="fileNamePattern" datatype="Regex">
                <description>%RegexpOnFilename.fileNamePattern</description>
            </property-metadata>
            <property-metadata name="match" datatype="Boolean" default-value="true">
                <description>%RegexpOnFilename.match</description>
            </property-metadata>
            <property-metadata name="ignoreFileNameExtensions" datatype="Boolean" default-value="false">
                <description>%RegexpOnFilename.ignoreFileNameExtensions</description>
            </property-metadata>
            <property-metadata name="fileExtensions" datatype="String">
                <description>%RegexpOnFilename.fileExtensions</description>
            </property-metadata>
            <message-key key="regexp.filename.match"/>
            <message-key key="regexp.filename.mismatch"/>
        </rule-metadata>

        <rule-metadata name="%RegexpSingleline.name" internal-name="RegexpSingleline" parent="Checker">
            <alternative-name internal-name="com.puppycrawl.tools.checkstyle.checks.regexp.RegexpSinglelineCheck"/>
            <description>%RegexpSingleline.desc</description>
            <property-metadata name="format" datatype="Regex" default-value="^$">
                <description>%RegexpSingleline.format</description>
            </property-metadata>
            <property-metadata name="message" datatype="String">
                <description>%RegexpSingleline.message</description>
            </property-metadata>
            <property-metadata name="ignoreCase" datatype="Boolean" default-value="false">
                <description>%RegexpSingleline.ignoreCase</description>
            </property-metadata>
            <property-metadata name="minimum" datatype="Integer" default-value="0">
                <description>%RegexpSingleline.minimum</description>
            </property-metadata>
            <property-metadata name="maximum" datatype="Integer" default-value="0">
                <description>%RegexpSingleline.maximum</description>
            </property-metadata>
            <property-metadata name="fileExtensions" datatype="String">
                <description>%RegexpSingleline.fileExtensions</description>
            </property-metadata>
            <message-key key="regexp.exceeded"/>
            <message-key key="regexp.minimum"/>
        </rule-metadata>

        <rule-metadata name="%RegexpSinglelineJava.name" internal-name="RegexpSinglelineJava" parent="TreeWalker">
            <alternative-name internal-name="com.puppycrawl.tools.checkstyle.checks.regexp.RegexpSinglelineJavaCheck"/>
            <description>%RegexpSinglelineJava.desc</description>
            <property-metadata name="format" datatype="Regex" default-value="^$">
                <description>%RegexpSinglelineJava.format</description>
            </property-metadata>
            <property-metadata name="message" datatype="String">
                <description>%RegexpSinglelineJava.message</description>
            </property-metadata>
            <property-metadata name="ignoreCase" datatype="Boolean" default-value="false">
                <description>%RegexpSinglelineJava.ignoreCase</description>
            </property-metadata>
            <property-metadata name="minimum" datatype="Integer" default-value="0">
                <description>%RegexpSinglelineJava.minimum</description>
            </property-metadata>
            <property-metadata name="maximum" datatype="Integer" default-value="0">
                <description>%RegexpSinglelineJava.maximum</description>
            </property-metadata>
            <property-metadata name="ignoreComments" datatype="Boolean" default-value="false">
                <description>%RegexpSinglelineJava.ignoreComments</description>
            </property-metadata>
            <message-key key="regexp.exceeded"/>
            <message-key key="regexp.minimum"/>
        </rule-metadata>

    </rule-group-metadata>

</checkstyle-metadata>
