<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE checkstyle-metadata PUBLIC
"-//eclipse-cs//DTD Check Metadata 1.1//EN"
"http://eclipse-cs.sourceforge.net/dtds/checkstyle-metadata_1_1.dtd">
<checkstyle-metadata>
    <rule-group-metadata name="%Naming.group" priority="200">

        <rule-metadata name="%AbstractClassName.name" internal-name="AbstractClassName" parent="TreeWalker">
            <alternative-name internal-name="com.puppycrawl.tools.checkstyle.checks.naming.AbstractClassNameCheck"/>
            <description>%AbstractClassName.desc</description>
            <property-metadata name="format" datatype="Regex" default-value="^Abstract.+$">
                <description>%AbstractClassName.format</description>
            </property-metadata>
            <property-metadata name="ignoreModifier" datatype="Boolean" default-value="false">
                <description>%AbstractClassName.ignoreModifier</description>
            </property-metadata>
            <property-metadata name="ignoreName" datatype="Boolean" default-value="false">
                <description>%AbstractClassName.ignoreName</description>
            </property-metadata>
            <message-key key="illegal.abstract.class.name"/>
            <message-key key="no.abstract.class.modifier"/>
        </rule-metadata>

        <rule-metadata name="%ClassTypeParameterName.name" internal-name="ClassTypeParameterName" parent="TreeWalker">
            <alternative-name internal-name="com.puppycrawl.tools.checkstyle.checks.naming.ClassTypeParameterNameCheck"/>
            <description>%ClassTypeParameterName.desc</description>
            <property-metadata name="format" datatype="Regex" default-value="^[A-Z]$">
                <description>%ClassTypeParameterName.format</description>
            </property-metadata>
            <message-key key="name.invalidPattern"/>
        </rule-metadata>

        <rule-metadata name="%ConstantName.name" internal-name="ConstantName" parent="TreeWalker">
            <alternative-name internal-name="com.puppycrawl.tools.checkstyle.checks.naming.ConstantNameCheck"/>
            <description>%ConstantName.desc</description>
            <property-metadata name="applyToPublic" datatype="Boolean" default-value="true">
                <description>%ConstantName.applyToPublic</description>
            </property-metadata>
            <property-metadata name="applyToProtected" datatype="Boolean" default-value="true">
                <description>%ConstantName.applyToProtected</description>
            </property-metadata>
            <property-metadata name="applyToPackage" datatype="Boolean" default-value="true">
                <description>%ConstantName.applyToPackage</description>
            </property-metadata>
            <property-metadata name="applyToPrivate" datatype="Boolean" default-value="true">
                <description>%ConstantName.applyToPrivate</description>
            </property-metadata>
            <property-metadata name="format" datatype="Regex" default-value="^[A-Z][A-Z0-9]*(_[A-Z0-9]+)*$">
                <description>%ConstantName.format</description>
            </property-metadata>
            <message-key key="name.invalidPattern"/>
        </rule-metadata>

        <rule-metadata name="%LocalFinalVariableName.name" internal-name="LocalFinalVariableName" parent="TreeWalker">
            <alternative-name internal-name="com.puppycrawl.tools.checkstyle.checks.naming.LocalFinalVariableNameCheck"/>
            <description>%LocalFinalVariableName.desc</description>
            <property-metadata name="format" datatype="Regex" default-value="^[a-z][a-zA-Z0-9]*$">
                <description>%LocalFinalVariableName.format</description>
            </property-metadata>
            <property-metadata name="tokens" datatype="MultiCheck" default-value="VARIABLE_DEF,PARAMETER_DEF,RESOURCE">
                <description>%LocalFinalVariableName.tokens</description>
                <enumeration>
                    <property-value-option value="VARIABLE_DEF"/>
                    <property-value-option value="PARAMETER_DEF"/>
                    <property-value-option value="RESOURCE"/>
                </enumeration>
            </property-metadata>
            <message-key key="name.invalidPattern"/>
        </rule-metadata>

        <rule-metadata name="%LocalVariableName.name" internal-name="LocalVariableName" parent="TreeWalker">
            <alternative-name internal-name="com.puppycrawl.tools.checkstyle.checks.naming.LocalVariableNameCheck"/>
            <description>%LocalVariableName.desc</description>
            <property-metadata name="format" datatype="Regex" default-value="^[a-z][a-zA-Z0-9]*$">
                <description>%LocalVariableName.format</description>
            </property-metadata>
            <property-metadata name="allowOneCharVarInForLoop" datatype="Boolean" default-value="false">
                <description>%LocalVariableName.allowOneCharVarInForLoop</description>
            </property-metadata>
            <message-key key="name.invalidPattern"/>
        </rule-metadata>

        <rule-metadata name="%MemberName.name" internal-name="MemberName" parent="TreeWalker">
            <alternative-name internal-name="com.puppycrawl.tools.checkstyle.checks.naming.MemberNameCheck"/>
            <description>%MemberName.desc</description>
            <property-metadata name="applyToPublic" datatype="Boolean" default-value="true">
                <description>%MemberName.applyToPublic</description>
            </property-metadata>
            <property-metadata name="applyToProtected" datatype="Boolean" default-value="true">
                <description>%MemberName.applyToProtected</description>
            </property-metadata>
            <property-metadata name="applyToPackage" datatype="Boolean" default-value="true">
                <description>%MemberName.applyToPackage</description>
            </property-metadata>
            <property-metadata name="applyToPrivate" datatype="Boolean" default-value="true">
                <description>%MemberName.applyToPrivate</description>
            </property-metadata>
            <property-metadata name="format" datatype="Regex" default-value="^[a-z][a-zA-Z0-9]*$">
                <description>%MemberName.format</description>
            </property-metadata>
            <message-key key="name.invalidPattern"/>
        </rule-metadata>

        <rule-metadata name="%MethodName.name" internal-name="MethodName" parent="TreeWalker">
            <alternative-name internal-name="com.puppycrawl.tools.checkstyle.checks.naming.MethodNameCheck"/>
            <description>%MethodName.desc</description>
            <property-metadata name="format" datatype="Regex" default-value="^[a-z][a-zA-Z0-9]*$">
                <description>%MethodName.format</description>
            </property-metadata>
            <property-metadata name="allowClassName" datatype="Boolean" default-value="false">
                <description>%MethodName.allowClassName</description>
            </property-metadata>
            <property-metadata name="applyToPublic" datatype="Boolean" default-value="true">
                <description>%MethodName.applyToPublic</description>
            </property-metadata>
            <property-metadata name="applyToProtected" datatype="Boolean" default-value="true">
                <description>%MethodName.applyToProtected</description>
            </property-metadata>
            <property-metadata name="applyToPackage" datatype="Boolean" default-value="true">
                <description>%MethodName.applyToPackage</description>
            </property-metadata>
            <property-metadata name="applyToPrivate" datatype="Boolean" default-value="true">
                <description>%MethodName.applyToPrivate</description>
            </property-metadata>
            <message-key key="name.invalidPattern"/>
            <message-key key="method.name.equals.class.name"/>
        </rule-metadata>

        <rule-metadata name="%MethodTypeParameterName.name" internal-name="MethodTypeParameterName" parent="TreeWalker">
            <alternative-name internal-name="com.puppycrawl.tools.checkstyle.checks.naming.MethodTypeParameterNameCheck"/>
            <description>%MethodTypeParameterName.desc</description>
            <property-metadata name="format" datatype="Regex" default-value="^[A-Z]$">
                <description>%MethodTypeParameterName.format</description>
            </property-metadata>
            <message-key key="name.invalidPattern"/>
        </rule-metadata>

        <rule-metadata name="%InterfaceTypeParameterName.name" internal-name="InterfaceTypeParameterName"
            parent="TreeWalker">
            <alternative-name
                internal-name="com.puppycrawl.tools.checkstyle.checks.naming.InterfaceTypeParameterNameCheck"/>
            <description>%InterfaceTypeParameterName.desc</description>
            <property-metadata name="format" datatype="Regex" default-value="^[A-Z]$">
                <description>%InterfaceTypeParameterName.format</description>
            </property-metadata>
            <message-key key="name.invalidPattern"/>
        </rule-metadata>

        <rule-metadata name="%PackageName.name" internal-name="PackageName" parent="TreeWalker">
            <alternative-name internal-name="com.puppycrawl.tools.checkstyle.checks.naming.PackageNameCheck"/>
            <description>%PackageName.desc</description>
            <property-metadata name="format" datatype="Regex" default-value="^[a-z]+(\.[a-zA-Z_][a-zA-Z0-9_]*)*$">
                <description>%PackageName.format</description>
            </property-metadata>
            <message-key key="name.invalidPattern"/>
        </rule-metadata>

        <rule-metadata name="%ParameterName.name" internal-name="ParameterName" parent="TreeWalker">
            <alternative-name internal-name="com.puppycrawl.tools.checkstyle.checks.naming.ParameterNameCheck"/>
            <description>%ParameterName.desc</description>
            <property-metadata name="format" datatype="Regex" default-value="^[a-z][a-zA-Z0-9]*$">
                <description>%ParameterName.format</description>
            </property-metadata>
            <property-metadata name="ignoreOverridden" datatype="Boolean" default-value="false">
                <description>%ParameterName.ignoreOverridden</description>
            </property-metadata>
            <property-metadata name="accessModifiers" datatype="SingleSelect" default-value="PUBLIC,PROTECTED,PACKAGE,PRIVATE">
                <description>%ParameterName.accessModifiers</description>
                <enumeration option-provider="com.puppycrawl.tools.checkstyle.checks.naming.AccessModifier"/>
            </property-metadata>
            <message-key key="name.invalidPattern"/>
        </rule-metadata>

        <rule-metadata name="%StaticVariableName.name" internal-name="StaticVariableName" parent="TreeWalker">
            <alternative-name internal-name="com.puppycrawl.tools.checkstyle.checks.naming.StaticVariableNameCheck"/>
            <description>%StaticVariableName.desc</description>
            <property-metadata name="applyToPublic" datatype="Boolean" default-value="true">
                <description>%StaticVariableName.applyToPublic</description>
            </property-metadata>
            <property-metadata name="applyToProtected" datatype="Boolean" default-value="true">
                <description>%StaticVariableName.applyToProtected</description>
            </property-metadata>
            <property-metadata name="applyToPackage" datatype="Boolean" default-value="true">
                <description>%StaticVariableName.applyToPackage</description>
            </property-metadata>
            <property-metadata name="applyToPrivate" datatype="Boolean" default-value="true">
                <description>%StaticVariableName.applyToPrivate</description>
            </property-metadata>
            <property-metadata name="format" datatype="Regex" default-value="^[a-z][a-zA-Z0-9]*$">
                <description>%StaticVariableName.format</description>
            </property-metadata>
            <message-key key="name.invalidPattern"/>
        </rule-metadata>

        <rule-metadata name="%TypeName.name" internal-name="TypeName" parent="TreeWalker">
            <alternative-name internal-name="com.puppycrawl.tools.checkstyle.checks.naming.TypeNameCheck"/>
            <description>%TypeName.desc</description>
            <property-metadata name="applyToPublic" datatype="Boolean" default-value="true">
                <description>%TypeName.applyToPublic</description>
            </property-metadata>
            <property-metadata name="applyToProtected" datatype="Boolean" default-value="true">
                <description>%TypeName.applyToProtected</description>
            </property-metadata>
            <property-metadata name="applyToPackage" datatype="Boolean" default-value="true">
                <description>%TypeName.applyToPackage</description>
            </property-metadata>
            <property-metadata name="applyToPrivate" datatype="Boolean" default-value="true">
                <description>%TypeName.applyToPrivate</description>
            </property-metadata>
            <property-metadata name="format" datatype="Regex" default-value="^[A-Z][a-zA-Z0-9]*$">
                <description>%TypeName.format</description>
            </property-metadata>
            <property-metadata name="tokens" datatype="MultiCheck"
                default-value="CLASS_DEF,INTERFACE_DEF,ENUM_DEF,ANNOTATION_DEF">
                <description>%TypeName.tokens</description>
                <enumeration>
                    <property-value-option value="CLASS_DEF"/>
                    <property-value-option value="INTERFACE_DEF"/>
                    <property-value-option value="ENUM_DEF"/>
                    <property-value-option value="ANNOTATION_DEF"/>
                </enumeration>
            </property-metadata>
            <message-key key="name.invalidPattern"/>
        </rule-metadata>

        <rule-metadata name="%AbbreviationAsWordInName.name" internal-name="AbbreviationAsWordInName"
            parent="TreeWalker">
            <alternative-name
                internal-name="com.puppycrawl.tools.checkstyle.checks.naming.AbbreviationAsWordInNameCheck"/>
            <description>%AbbreviationAsWordInName.desc</description>
            <property-metadata name="allowedAbbreviationLength" datatype="Integer" default-value="3">
                <description>%AbbreviationAsWordInName.allowedAbbreviationLength</description>
            </property-metadata>
            <property-metadata name="allowedAbbreviations" datatype="String">
                <description>%AbbreviationAsWordInName.allowedAbbreviations</description>
            </property-metadata>
            <property-metadata name="ignoreFinal" datatype="Boolean" default-value="true">
                <description>%AbbreviationAsWordInName.ignoreFinal</description>
            </property-metadata>
            <property-metadata name="ignoreStatic" datatype="Boolean" default-value="true">
                <description>%AbbreviationAsWordInName.ignoreStatic</description>
            </property-metadata>
            <property-metadata name="ignoreOverriddenMethods" datatype="Boolean" default-value="true">
                <description>%AbbreviationAsWordInName.ignoreOverriddenMethods</description>
            </property-metadata>
            <property-metadata name="tokens" datatype="MultiCheck"
                default-value="CLASS_DEF,INTERFACE_DEF,ENUM_DEF,ANNOTATION_DEF,ANNOTATION_FIELD_DEF,PARAMETER_DEF,VARIABLE_DEF,METHOD_DEF">
                <description>%AbbreviationAsWordInName.tokens</description>
                <enumeration>
                    <property-value-option value="CLASS_DEF"/>
                    <property-value-option value="INTERFACE_DEF"/>
                    <property-value-option value="ENUM_CONSTANT_DEF"/>
                    <property-value-option value="ENUM_DEF"/>
                    <property-value-option value="ANNOTATION_DEF"/>
                    <property-value-option value="ANNOTATION_FIELD_DEF"/>
                    <property-value-option value="PARAMETER_DEF"/>
                    <property-value-option value="VARIABLE_DEF"/>
                    <property-value-option value="METHOD_DEF"/>
                </enumeration>
            </property-metadata>
            <message-key key="abbreviation.as.word"/>
        </rule-metadata>

        <rule-metadata name="%CatchParameterName.name" internal-name="CatchParameterName" parent="TreeWalker">
            <alternative-name internal-name="com.puppycrawl.tools.checkstyle.checks.naming.CatchParameterNameCheck"/>
            <description>%CatchParameterName.desc</description>
            <property-metadata name="format" datatype="Regex" default-value="^(e|t|ex|[a-z][a-z][a-zA-Z]+)$">
                <description>%CatchParameterName.format</description>
            </property-metadata>
            <message-key key="name.invalidPattern"/>
        </rule-metadata>

        <rule-metadata name="%LambdaParameterName.name" internal-name="LambdaParameterName" parent="TreeWalker">
            <alternative-name internal-name="com.puppycrawl.tools.checkstyle.checks.naming.LambdaParameterNameCheck"/>
            <description>%LambdaParameterName.desc</description>
            <property-metadata name="format" datatype="Regex" default-value="^[a-z][a-zA-Z0-9]*$">
                <description>%LambdaParameterName.format</description>
            </property-metadata>
            <message-key key="name.invalidPattern"/>
        </rule-metadata>
    </rule-group-metadata>

</checkstyle-metadata>
