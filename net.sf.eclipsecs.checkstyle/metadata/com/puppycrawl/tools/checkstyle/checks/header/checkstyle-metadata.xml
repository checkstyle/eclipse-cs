<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE checkstyle-metadata PUBLIC
"-//eclipse-cs//DTD Check Metadata 1.1//EN"
"http://eclipse-cs.sourceforge.net/dtds/checkstyle-metadata_1_1.dtd">
<checkstyle-metadata>
    <rule-group-metadata name="%Headers.group" priority="300">

        <rule-metadata name="%Header.name" internal-name="Header" parent="Checker">
            <alternative-name internal-name="com.puppycrawl.tools.checkstyle.checks.header.HeaderCheck"/>
            <description>%Header.desc</description>
            <property-metadata name="headerFile" datatype="File">
                <description>%Header.headerFile</description>
            </property-metadata>
            <property-metadata name="charset" datatype="String">
                <description>%Header.charset</description>
            </property-metadata>
            <property-metadata name="header" datatype="String">
                <description>%Header.header</description>
            </property-metadata>
            <property-metadata name="ignoreLines" datatype="String">
                <description>%Header.ignoreLines</description>
            </property-metadata>
            <property-metadata name="fileExtensions" datatype="String">
                <description>%Header.fileExtensions</description>
            </property-metadata>
            <message-key key="header.missing"/>
            <message-key key="header.mismatch"/>
        </rule-metadata>

        <rule-metadata name="%RegexpHeader.name" internal-name="RegexpHeader" parent="Checker">
            <alternative-name internal-name="com.puppycrawl.tools.checkstyle.checks.header.RegexpHeaderCheck"/>
            <description>%RegexpHeader.desc</description>
            <property-metadata name="headerFile" datatype="File">
                <description>%RegexpHeader.headerFile</description>
            </property-metadata>
            <property-metadata name="charset" datatype="String">
                <description>%RegexpHeader.charset</description>
            </property-metadata>
            <property-metadata name="header" datatype="String">
                <description>%RegexpHeader.header</description>
            </property-metadata>
            <property-metadata name="multiLines" datatype="String">
                <description>%RegexpHeader.multiLines</description>
            </property-metadata>
            <property-metadata name="fileExtensions" datatype="String">
                <description>%RegexpHeader.fileExtensions</description>
            </property-metadata>
            <message-key key="header.missing"/>
            <message-key key="header.mismatch"/>
        </rule-metadata>

    </rule-group-metadata>
</checkstyle-metadata>
