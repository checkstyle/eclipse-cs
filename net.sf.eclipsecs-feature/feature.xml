<?xml version="1.0" encoding="UTF-8"?>
<feature
      id="net.sf.eclipsecs"
      label="%feature.label"
      version="10.13.0.qualifier"
      provider-name="%feature.provider_name"
      plugin="net.sf.eclipsecs.branding">

   <description url="%description.url">
      %description.text
   </description>

   <copyright>
      %copyright.text
   </copyright>

   <license url="license.html">
      %license.text
   </license>

   <url>
      <update label="%updateSiteName" url="https://checkstyle.org/eclipse-cs-update-site/"/>
   </url>

   <requires>
      <import plugin="org.eclipse.core.runtime" version="3.22.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.core.resources"/>
      <import plugin="org.eclipse.jdt.core"/>
      <import plugin="org.eclipse.team.core"/>
      <import plugin="org.eclipse.core.filebuffers"/>
      <import plugin="org.eclipse.jface.text"/>
      <import plugin="org.eclipse.jdt.ui"/>
      <import plugin="org.eclipse.ui"/>
      <import plugin="org.eclipse.ui.editors"/>
      <import plugin="org.eclipse.ui.ide"/>
      <import plugin="org.eclipse.ui.workbench.texteditor"/>
   </requires>

   <plugin
         id="net.sf.eclipsecs.branding"
         version="10.13.0.qualifier"/>

   <plugin
         id="net.sf.eclipsecs.core"
         version="10.13.0.qualifier"/>

   <plugin
         id="net.sf.eclipsecs.doc"
         version="10.13.0.qualifier"/>

   <plugin
         id="net.sf.eclipsecs.checkstyle"
         version="10.13.0.qualifier"/>

   <plugin
         id="net.sf.eclipsecs.ui"
         version="10.13.0.qualifier"/>

</feature>
